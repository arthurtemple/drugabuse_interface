                                    <Path x:Name="path" Data="F1M8.3088,-0.838200000000001L0.000799999999999912,7.4708L17.6098,25.0808L-0.000199999999999534,42.6908L8.3088,50.9998L25.9188,33.3898L43.5298,50.9998L51.8378,42.6908L34.2278,25.0818L51.8378,7.4718L43.5298,-0.837200000000001L25.9188,16.7718z" Fill="{StaticResource GUIColor-Default}" Height="51.838" Canvas.Left="0" Stretch="Fill" Canvas.Top="0" Width="51.838" HorizontalAlignment="Center"/>
                                </Canvas>
                            </Canvas>
                        </Viewbox>
                    </Grid>
                    <ControlTemplate.Triggers>
                      
